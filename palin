//responsible for converting int to str
let rec f n =
n eq 0 -> '0' |
n eq 1 -> '1'  |
n eq 2 -> '2'|
n eq 3 -> '3'|
n eq 4 -> '4' |
n eq 5 -> '5' |
n eq 6 -> '6' |
n eq 7 -> '7' |
n eq 8 -> '8' |
n eq 9 -> '9' |
Conc (f(n/10)) (f(n-n/10*10))
in
//palindrome
let rec r s  = s eq ''->'' | Conc (r (Stern s)) (Stem s)
within
p s = not Isstring s -> 'error' | s eq r s
in
Print(p (f(12421)))
